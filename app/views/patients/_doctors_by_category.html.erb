  <% @doctors_by_category.each do |d| %>
  <div class="list-group">
    <div class="d-flex w-400 justify-content-between">
      <h5 class="mb-1"><%= d.u_name %></h5>
      <small><%= button_to "Appointment", '/appointments/new/'+d.d_id.to_s, method: :get, class: "btn btn-primary" %></small>
    </div>
    <p class="mb-1"><%= d.dc_name %></p>
  </div>
  <% end %>
